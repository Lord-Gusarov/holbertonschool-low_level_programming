#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 *main - prints a number and wether the last digit is greater than 5, less than
 * 6 or zero
 *
 *Return: 0 always zero
 */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */

	int d = n % 10;

	if (d > 0)
		printf("Last digit of %i is %i ", n, d);
	else if (d > 5)
		puts("greater than 5");
	else
		puts("less than 6 and not 0");

	return (0);
}
